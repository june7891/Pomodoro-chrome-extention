{
    "manifest_version": 3,
    "name": "Time's up!",
    "version": "1.0",
    "description": "Pomodoro timer extention - Help you focus on your task",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_title": "Time's up!",
        "default_popup": "popup/popup.html"

    },
    "permissions": ["storage", "notifications"]
}